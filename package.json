{
  "name": "improviz-web",
  "version": "0.0.1",
  "description": "A web version of improviz",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:improviz && npm run build:site",
    "start": "npm run start:improviz",
    "build:improviz": "webpack --mode production",
    "build:site": "npx @11ty/eleventy",
    "start:improviz": "webpack-dev-server --mode development",
    "start:site": "npx @11ty/eleventy --serve --port 8081",
    "test": "jest test"
  },
  "bugs": {
    "url": "https://github.com/rumblesan/improviz-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rumblesan/improviz-web"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.10.0",
    "@babel/core": "7.8.6",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "file-loader": "5.1.0",
    "handlebars": "4.7.3",
    "handlebars-loader": "1.7.1",
    "jest": "25.1.0",
    "js-yaml-loader": "^1.2.2",
    "mini-css-extract-plugin": "0.9.0",
    "prettier": "1.19.1",
    "sass": "1.26.1",
    "sass-loader": "8.0.2",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.3",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "dependencies": {
    "@rumblesan/virgil": "0.0.7",
    "dentist": "1.0.3"
  },
  "jest": {
    "setupFiles": [
      "./test/helpers.js"
    ]
  },
